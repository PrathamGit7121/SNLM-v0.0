<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="addressBean1" class="famt.myPack.Address">
		<constructor-arg index="0" value="101"></constructor-arg>
		<constructor-arg index="1" type="String" value="M M"></constructor-arg>
		<constructor-arg index="2" value="Rtn"></constructor-arg>
		<constructor-arg index="3" value="415612"></constructor-arg>
	</bean>

	<bean id="studBean1" class="famt.myPack.Student">
		<constructor-arg index="0" value="1"></constructor-arg>
		<constructor-arg index="1" type="String" value="Aadesh"></constructor-arg>
		<constructor-arg index="2" value="85"></constructor-arg>
		<constructor-arg>
			<ref bean="addressBean1"/>
		</constructor-arg>
	</bean>
</beans>